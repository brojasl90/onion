@using Infrastructure.Models
@model IEnumerable<Infrastructure.Models.GestionInventario>

<div class="card text-dark mb-3 p-1" style="border-bottom-left-radius: 8px; border-bottom-right-radius: 8px;">
    <div class="card-header text-white bg-primary bg-gradient">Entradas registradas para hoy.</div>
    <div class="card-body bg-light">
        <h5 class="card-title">
            Total: @{
                Html.RenderAction("_TotalEntradas", "Inventario");
            } entradas.
        </h5>
    </div>
</div>
